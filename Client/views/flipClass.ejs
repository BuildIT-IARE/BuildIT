    <style>
      .green {
        color: green;
      }
      .red {
        color: red;
      }
      .orange {
        color: orange;
      }
      .black {
        color: black;
      }

      .button1 {
        border-radius: 4px;
        background-color: #454fd4;
        border: none;
        color: white;
        text-align: center;
        font-size: 15px;
        padding: 5px;
        width: 110px;
        transition: all 0.5s;
        cursor: pointer;
        margin: 5px;
      }

      .button1 span {
        cursor: pointer;
        display: inline-block;
        transition: 1s;
      }

      .button1 span:after {
        content: "\21B6";
        opacity: 0;
        top: 50px;
        right: 20px;
        transition: 0.3s;
      }

      .button1:hover span {
        padding-right: 20px;
      }

      .button1:hover span:after {
        opacity: 1;
        right: 0;
        padding-bottom: 20px;
      }
      p {
        font-size: 130%;
      }
      .button {
        border-radius: 4px;
        background-color: #a62232;
        border: none;
        color: white;
        /* text-align: center; */
        font-size: px;
        padding: 20px;
        width: 160px;
        transition: all 0.5s;
        cursor: pointer;
        margin: 5px;
      }
      .button span {
        cursor: pointer;
        display: inline-block;
        position: relative;
        transition: 1s;
        background-color: #a62232;
      }
      .button span:after {
        content: "\00bb";
        position: "relative";
        opacity: 0;
        top: 0;
        right: -20px;
        transition: 0.5s;
        background-color: #a62232;
      }
      .button:hover span {
        padding-left: 25px;
      }
      .button:hover span:after {
        opacity: 1;
        right: 0;
      }
      div .border {
        border-color: red;
        margin: 4px;
        padding: 4px;
        border-width: 3px;
      }

      .button2 {
        border-radius: 4px;
        background-color: #454fd4;
        border: none;
        color: white;
        /* text-align: center; */
        font-size: px;
        padding: 20px;
        width: 100%;
        transition: all 0.5s;
        cursor: pointer;
        margin: 5px;
      }
      .button2 span {
        cursor: pointer;
        display: inline-block;
        position: relative;
        transition: 1s;
      }
      .button2 span:after {
        content: "\00bb";
        position: "relative";
        opacity: 0;
        top: 0;
        right: -20px;
        transition: 0.5s;
      }
      .button2:hover span {
        padding-left: 25px;
      }
      .button2:hover span:after {
        opacity: 1;
        right: 0;
      }
      div .border {
        border-color: red;
        margin: 4px;
        padding: 4px;
        border-width: 3px;
      }
    </style>
