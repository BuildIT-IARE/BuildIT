          function updatePassword(){

            var alert = document.getElementById("alert")
            

            let pass = document.getElementById("pass").value
            let cpass  = document.getElementById("cpass").value
            let secret = document.getElementById("secret").value
            if (pass===cpass && secret && pass.length>=8){
            try{
              var url = "<%= serverUrl%>/updatePassword"
              $.ajax({
                // data: { arr: body1 },
                async: false,
                // enctype: 'multipart/form-data',
                url: url,
                headers: { "Authorization": "<%=token%>" },
                type: "POST",
                data: JSON.stringify({password:pass, cpassword:cpass, secret:secret, username:"<%=imgUsername%>"}),
                cache: false,
                contentType: "application/json",
                processData: false,
                dataType: 'json',
                success: function (response) {
                  alert.innerHTML='<span class="material-icons-outlined">check_circle</span>'+response.message
                  alert.style.transform="rotateY(0deg)"
                  alert.style.background="rgb(170,249,0)"
                  alert.style.color="black"
                  setTimeout(()=>{
                    alert.style.transform="rotateY(90deg)"
                    alert.style.background="black"
                    alert.style.color="rgb(170,249,0)"
                  },5000)
                },
                error: function (error) {
                  console.log(`Error ${JSON.stringify(error)}`);
                  alert.innerHTML="Some error occurred"
                  alert.style.transform="rotateY(0deg)"
                  setTimeout(()=>{
                    alert.style.transform="rotateY(90deg)"
                  },5000)
                }
              })
              }
            catch (err) {
              console.log(err);
              alert.innerHTML="Some error occurred"
              alert.style.transform="rotateY(0deg)"
              setTimeout(()=>{
                alert.style.transform="rotateY(90deg)"
              },5000)
            }
            }
            else{
              if (secret==""){
                alert.innerHTML="Secret is not entered"
              }
              else if(pass.length<8){
                alert.innerHTML="Password is too short"
              }
              else if (secret.length!=32){
                alert.innerHTML="Entered wrong secret"
              }
              else{
                alert.innerHTML="Passwords did not match"
              }
              alert.style.transform="rotateY(0deg)"
              setTimeout(()=>{
                alert.style.transform="rotateY(90deg)"
              },5000)
            }
            return false
          }
        </script>
      </section>

