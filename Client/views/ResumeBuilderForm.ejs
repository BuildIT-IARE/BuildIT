    <section class="resumeform">
      <form action="<%= url%>/resume" method="post" id="myForm">
        <input
          type="text"
          name="username"
          style="display: none"
          value="<%= imgUsername %>"
        />
        <span class="formsection">
          <span class="content">
            <span class="sectionheading">Heading</span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="text"
                  name="Fname"
                  id="Fname"
                  class="inputf"
                  placeholder="First Name"
                  required
                />
                <span class="label">First Name</span>
              </span>
              <span class="inputfield">
                <input
                  type="text"
                  name="Lname"
                  id="Lname"
                  class="inputf"
                  placeholder="Last Name"
                  required
                />
                <span class="label">Last Name</span>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="text"
                  name="City"
                  id="City"
                  placeholder="City"
                  class="inputf"
                  required
                />
                <span class="label">City</span>
              </span>
              <span class="inputfield">
                <input
                  type="text"
                  name="Country"
                  id="Country"
                  placeholder="Country"
                  class="inputf"
                  required
                />
                <span class="label">Country</span>
              </span>
              <span class="inputfield">
                <input
                  type="number"
                  name="PinCode"
                  id="PinCode"
                  placeholder="Pin Code"
                  class="inputf"
                  required
                />
                <span class="label">Pin Code</span>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="number"
                  name="Phone"
                  id="Phone"
                  placeholder="Phone"
                  class="inputf"
                  required
                />
                <span class="label">Phone</span>
              </span>
              <span class="inputfield">
                <input
                  type="email"
                  name="Email"
                  id="Email"
                  placeholder="Email"
                  class="inputf"
                  required
                />
                <span class="label">Email</span>
              </span>
            </span>
            <button type="button" class="socialbutton" id="sb1">
              <span class="material-icons"> add_circle_outline </span>
              Add Social Media Links
            </button>
            <% for(var i=1;i<=3;i++){ %>
            <span class="formspan" id="socialspan<%=i%>">
              <span class="inputfield">
                <input
                  type="text"
                  name="Social<%= i %>"
                  placeholder="Social Website"
                  class="inputf"
                />
                <span class="label">Social Website</span>
              </span>
              <span class="inputfield">
                <input
                  type="url"
                  name="SocialURL<%= i %>"
                  placeholder="Social URL"
                  class="inputf"
                />
                <span class="label">Social URL</span>
              </span>
            </span>
            <% } %>
            <span class="buttons">
              <button type="button" class="buttonf" id="pre1">Back</button>
              <button type="button" class="buttonf" id="nex1">Next</button>
            </span>
          </span>
        </span>
        <span class="formsection2">
          <span class="content">
            <span class="sectionheading">Education</span>
            <span class="sublabel">Schooling Details</span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="text"
                  name="SchoolName"
                  id="SchoolName"
                  placeholder="School Name"
                  class="inputf"
                  required
                />
                <span class="label">School Name</span>
              </span>
              <span class="inputfield">
                <input
                  type="text"
                  name="SchoolLocation"
                  id="SchoolLocation"
                  placeholder="School Location"
                  class="inputf"
                  required
                />
                <span class="label">School Location</span>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="month"
                  name="ScDateofPass"
                  id="ScDateofPass"
                  placeholder="Date of Pass"
                  class="inputf"
                  required
                />
                <span class="label">Date of Pass</span>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="number"
                  name="ScFinalScore"
                  id="ScFinalScore"
                  placeholder="Final Score"
                  class="inputf"
                  step="any"
                  required
                />
                <span class="label">Final Score</span>
              </span>
              <span class="inputfield">
                <select
                  name="ScMarksType"
                  id="ScMarksType"
                  class="inputf"
                  required
                >
                  <option value="" style="display: none">Select Type</option>
                  <option value="Percentage">Percentage</option>
                  <option value="CGPA">CGPA</option>
                </select>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="text"
                  name="ScCourseDesc"
                  id="ScCourseDesc"
                  class="inputf fullwidth"
                  required
                  placeholder="Class/Course Description (ex: Class 10 (SSC))"
                />
                <span class="label">Class/Course Description</span>
              </span>
            </span>
            <span class="sublabel">Intermediate / Diploma Details</span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="text"
                  name="IntCollegeName"
                  id="IntCollegeName"
                  placeholder="College Name"
                  class="inputf"
                  required
                />
                <span class="label">College Name</span>
              </span>
              <span class="inputfield">
                <input
                  type="text"
                  name="IntColllegeLocation"
                  id="IntColllegeLocation"
                  placeholder="College Location"
                  class="inputf"
                  required
                />
                <span class="label">College Location</span>
              </span>
            </span>
            <span class="formspan">
              <span class="inputfield">
                <input
                  type="month"
                  name="IntDateofStart"
                  id="IntDateofStart"
                  placeholder="Date of Start"
                  class="inputf"
                  required
                />
                <span class="label">Date of Start</span>
              </span>
              <span class="inputfield">
                <input
                  type="month"
                  name="IntDateofPass"
