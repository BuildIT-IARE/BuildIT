      var toggle = 0;
      var press = 0;
      dots.onclick = function () {
        if (toggle == 0) {
          dotsmenu.style.display = "flex";
          toggle = 1;
        } else {
          dotsmenu.style.display = "none";
          toggle = 0;
        }
        press = 1;
