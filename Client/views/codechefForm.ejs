        height: 6px;
        background: rgb(230, 230, 230);
        position: absolute;
        top: 0;
        display: none;
      }
      .loading {
        width: 100%;
        height: 100%;
        animation: move linear 2s infinite;
      }
      .load-bar {
        width: 15%;
        height: 100%;
        background: rgb(11, 133, 255);
        animation: load linear 2s infinite;
      }
      @keyframes load {
        0% {
          width: 15%;
        }
        70% {
          width: 30%;
        }
        100% {
          width: 27.5%;
        }
      }
      @keyframes move {
        0% {
          transform: translateX(0%);
        }
        100% {
          transform: translateX(100%);
        }
      }
      #mark-link {
        display: none;
      }
      .error .label {
        color: red;
      }
      .error input:focus {
        outline: red solid 2px;
      }
      .error input {
        border: red 1px solid;
      }
      .error select:focus {
        outline: red solid 2px;
      }
      .error select {
