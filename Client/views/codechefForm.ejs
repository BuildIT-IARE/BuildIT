        margin-right: 5px;
      }
      .form-input,
      .day,
      .month,
      .year,
      .form-duration,
      .form-rating,
      .form-link {
        position: relative;
      }
      .label {
        position: absolute;
        top: 0px;
        left: 5px;
        font-size: 12px;
        font-family: poppins;
        color: rgb(1, 120, 238);
        opacity: 0;
        transition: 0.25s;
      }
      .inputs:focus + .label {
        opacity: 1;
        top: -20px;
        left: 0;
      }
      .inputs:focus::placeholder {
        opacity: 0;
      }
      .inputs:not(:placeholder-shown) + .label {
        opacity: 1;
        top: -25px;
        left: 0;
      }
      .inputs:focus {
        outline: rgb(1, 120, 238) solid 2px;
        border: none;
      }
      .inputs2:focus + .label {
        opacity: 1;
        top: -20px;
        left: 0;
      }
      .inputs2:focus::placeholder {
        opacity: 0;
      }
      .inputs2:not(:placeholder-shown) + .label {
        opacity: 1;
        top: -25px;
        left: 0;
      }
      .inputs2:focus {
        outline: rgb(1, 120, 238) solid 2px;
        border: none;
      }
      .alert {
        position: absolute;
        top: 32px;
        left: 0;
        font-size: 11px;
        font-family: poppins;
        color: red;
        opacity: 0;
        display: flex;
        align-items: center;
